n=int(input("Number that has to be checked"))

def primeornot(a):
    x=0
    
    for i in range(1,a):
            if a%i==0:
                  x+=i
    
    if x==1:
         print(n,"Is a prime number")
    else:print(n,"Is not a prime number")



primeornot(n)
        

   

